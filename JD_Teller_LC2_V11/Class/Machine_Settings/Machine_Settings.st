    if    _FirstScan then

          rComp_Teller := rComp_Teller.Read();
          rComp_Ventieldriver := rComp_Ventieldriver.Read();
          rComp_FreeJam := rComp_FreeJam.Read();
          rComp_JamTeachIn := rComp_JamTeachIn.Read();
          rOpt_Operator_Login := rOpt_Operator_Login.Read();


