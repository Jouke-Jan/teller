          Comp_JamTeachIn.Write(input:=rComp_JamTeachIn);
          Opt_Operator_Login.Write(input:=rOpt_Operator_Login);
          Comp_Teller := 1;
          cComp_Teller.Write(Comp_Teller);
          
    end_if;


