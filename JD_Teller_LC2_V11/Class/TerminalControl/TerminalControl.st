FUNCTION VIRTUAL GLOBAL TerminalControl::IF_WindowRun
	VAR_INPUT
		whoami 	: ^_WHOAMI;
		pic 	: ^void;
		event 	: ^_EVENT;
	END_VAR

// check screen number..
	case whoami^.ima of

	IMA_PICTURE:
		ActScreenNbr := to_dint whoami^.no;

	IMA_WINDOW:
		ActWindowNbr := to_dint whoami^.no;
	end_case;

//  key control
	if event^.ftype = _event_keypress then
		KeyCode := event^.scancode;
  case event^.scancode of
			_F1: F1 := 1;
			_F2: F2 := 1;
			_F3: F3 := 1;
			_F4: F4 := 1;
			_F5: F5 := 1;
			_F6: F6 := 1;
			_F7: F7 := 1;
			_F8: F8 := 1;
			_UP: Up := 1;
			_DOWN : Down := 1;
			_LEFT : Left := 1;
			_RIGHT: Right := 1;
			_PGUP : PgUp := 1;
			_PGDOWN : PgDown := 1;
		end_case;
	elsif event^.ftype = _event_keyrelease then
		case event^.scancode of
			_F1: F1 := 0;
			_F2: F2 := 0;
			_F3: F3 := 1;
			_F4: F4 := 0;
			_F5: F5 := 0;
			_F6: F6 := 0;
			_F7: F7 := 0;
			_F8: F8 := 0;
			_UP: Up := 0;
			_DOWN : Down := 0;
			_LEFT : Left := 0;
			_RIGHT: Right := 0;
			_PGUP : PgUp := 0;
			_PGDOWN : PgDown := 0;
		end_case;
    
		keypresCnt += 1;
	end_if;

  if (ActScreenNbr = 0 & SelectScreen = -1 & cNextScreen > 0) then
    if ops.tAbsolute - TimeMark >= 40 * _100MS then
      SelectScreen := cNextScreen;
    end_if;  
  else
    TimeMark := ops.tAbsolute;
  end_if;  
  

END_FUNCTION //VIRTUAL GLOBAL MyGlobal::IF_WindowRun

